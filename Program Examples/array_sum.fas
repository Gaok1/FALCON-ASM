.data
arr:    .word 1, 2, 3, 4, 5, 6
prefix: .asciz "Sum array: "

.text
    la t0, arr
    li t1, 6       # length
    li s0, 0       # sum
    li t2, 0       # i

sum_loop:
    slli t3, t2, 2
    add  t3, t0, t3
    lw   t4, 0(t3)
    add  s0, s0, t4
    addi t2, t2, 1
    blt  t2, t1, sum_loop

    printString prefix
    print s0
    halt

